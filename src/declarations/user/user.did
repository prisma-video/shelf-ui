type UserProfile = 
 record {
   communities: opt vec text;
   doubleOptIn: bool;
   emailAddress: opt text;
   firstName: opt text;
   lastName: opt text;
   userName: opt text;
 };
type Role = 
 variant {
   admin;
   ambassador;
   authorized;
   guest;
 };
service : {
  assign_role: (principal, Role) -> ();
  createNewUser: (UserProfile) -> ();
  getUserProfile: () -> (opt UserProfile);
  getUserRole: () -> (opt Role);
  updateUserProfile: (UserProfile) -> ();
}
