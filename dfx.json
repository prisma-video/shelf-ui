{
  "canisters": {
    "user": {
      "main": "src/user/user.mo",
      "type": "motoko"
    },
    "nft": {
      "main": "src/nft/nft.mo",
      "type": "motoko"
    },
    "shelf_ui": {
      "dependencies": [
        "user",
        "nft"
      ],
      "frontend": {
        "entrypoint": "src/shelf_ui/public/index.html"
      },
      "source": [
        "src/shelf_ui/assets",
        "src/shelf_ui/src"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "output": "src/declarations/",
      "packtool": ""
    }
  },
  "dfx": "0.8.1",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}