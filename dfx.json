{
  "canisters": {
    "user": {
      "main": "backend/dfinity/user/user.mo",
      "type": "motoko"
    },
    "nft": {
      "main": "backend/dfinity/nft/nft.mo",
      "type": "motoko"
    },
    "swap_contracts": {
      "main": "backend/dfinity/swap_contracts/swap_contracts.mo",
      "type": "motoko"
    },
    "bigmap": {
      "type": "custom",
      "candid": "vendor/bigmap-rs/src/bigmap_index.did",
      "wasm": "vendor/bigmap-rs/target/wasm32-unknown-unknown/release/bigmap_index.wasm",
      "build": "./vendor/bigmap-rs/build"
    },
    "bigmap_data": {
      "type": "custom",
      "candid": "vendor/bigmap-rs/src/bigmap_data.did",
      "wasm": "vendor/bigmap-rs/target/wasm32-unknown-unknown/release/bigmap_data.wasm",
      "build": "./vendor/bigmap-rs/build"
    },
    "metadata_db": {
        "type": "custom",
        "build": "cargo build --target wasm32-unknown-unknown --package graphql --release --target-dir backend/sudograph/",
        "optimize": "ic-cdk-optimizer backend/sudograph/wasm32-unknown-unknown/release/graphql.wasm -o backend/sudograph/wasm32-unknown-unknown/release/graphql-opt.wasm",
        "candid": "backend/dfinity/metadata_db/src/graphql.did",
        "wasm": "backend/sudograph/wasm32-unknown-unknown/release/graphql-opt.wasm"
    },
    "shelf_ui": {
      "dependencies": [
        "user",
        "nft",
        "metadata_db",
        "swap_contracts",
        "bigmap",
        "bigmap_data"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": [
        "dist/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "output": "src/backend/dfinity/",
      "packtool": ""
    },
    "start": {
      "address": "127.0.0.1",
      "port": 8000,
      "serve_root": "canisters/eeoo/assets"
    }
  },
  "dfx": "0.8.1",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}